<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="View.ProfileMenu"
            prefHeight="720.0" prefWidth="1280.0"
            stylesheets="@../CSS/style4.css"
            styleClass="background">

    <center>
        <VBox alignment="CENTER" maxWidth="300" maxHeight="300" spacing="30">
            <Label text=" " fx:id="passwordLabel"/>
            <TextField promptText="enter your new password" styleClass="text-field" prefWidth="250" />
            <Button text="change" styleClass="button" prefWidth="250" onMouseClicked="#changePass"/>
            <Button text="profile menu" styleClass="button" prefWidth="250" onMouseClicked="#switchToProfileMenu"/>
        </VBox>
    </center>

</BorderPane>
