<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Text?>
<BorderPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="View.CreateHost"
            prefHeight="720.0" prefWidth="1280.0" styleClass="background" stylesheets="@../CSS/style6.css">
    <center>
        <VBox alignment="CENTER" spacing="10" maxWidth="350">
            <Label text="enter nicknames down bellow"/>
            <TextField fx:id="names" promptText="nickname 1,nickname 2, ..."/>
            <Button text="Create" onMouseClicked="#createClicked"/>
            <Text text=" "/>
            <Button styleClass="backButton" text="Back" onMouseClicked="#backClicked"/>
        </VBox>
    </center>

</BorderPane>
